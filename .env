# Environment Variables Template
# Copy this to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_BASE_URL=https://influencer-marketing-api.notebrains.com/
NEXT_PUBLIC_PAYMENT_URL=https://your-payment-service.com/
NEXT_PUBLIC_PAYMENT_TOKEN=your_payment_token_here

# AWS S3 Configuration
NEXT_PUBLIC_AWS_REGION=us-east-1
NEXT_PUBLIC_AWS_S3_BUCKET=your_bucket_name
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key

# Optional: Analytics or other services
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id
